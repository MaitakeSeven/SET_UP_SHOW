<h1>sessions#new</h1>
<p>Find me in app/views/users/show.html.erb</p>
<%= form_with(url: login_path, scope: :session,local: true) do |f| %>

    <%= f.label :email,"Email" %>
    <%= f.email_field :email %>
    
    <%= f.label :password, "password"%>
    <%= f.password_field :password %>
    
    <%= f.submit "Log in" %>

<% end %>